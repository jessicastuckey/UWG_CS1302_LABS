<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="458.0" minWidth="500.0" prefHeight="592.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs1302.project1jessicastuckey.view.MainWindow">
   <children>
      <ListView fx:id="pantryListView" layoutX="86.0" layoutY="240.0" prefHeight="297.0" prefWidth="328.0" />
      <ComboBox fx:id="type" layoutX="240.0" layoutY="81.0" prefWidth="150.0" />
      <TextField fx:id="name" layoutX="240.0" layoutY="32.0" prefHeight="26.0" prefWidth="150.0" />
      <Label layoutX="145.0" layoutY="28.0" prefHeight="34.0" prefWidth="47.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="145.0" layoutY="84.0" text="Type">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="221.0" layoutY="196.0" mnemonicParsing="false" onAction="#addGrocery" text="Add" />
      <Button layoutX="426.0" layoutY="240.0" mnemonicParsing="false" onAction="#removeGrocery" text="Remove" />
      <Label layoutX="142.0" layoutY="142.0" text="Quantity">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="quantity" layoutX="240.0" layoutY="139.0" />
      <Button layoutX="401.0" layoutY="119.0" mnemonicParsing="false" onAction="#increaseQuantity" text="+" />
      <Button layoutX="401.0" layoutY="155.0" mnemonicParsing="false" onAction="#decreaseQuantity" prefHeight="26.0" prefWidth="25.0" text="-" /> 
   </children>
</AnchorPane>
